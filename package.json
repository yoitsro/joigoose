{
  "name": "joigoose",
  "version": "6.1.1",
  "description": "Joi validation for your Mongoose models without the hassle of maintaining two schemas",
  "main": "lib/index.js",
  "scripts": {
    "test": "lab test -c -I queueMicrotask",
    "release": "standard-version",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoitsro/joigoose"
  },
  "keywords": [
    "joi",
    "mongoose",
    "validation",
    "schema",
    "model",
    "hapi"
  ],
  "author": "Ro Ramtohul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoitsro/joigoose/issues"
  },
  "homepage": "https://github.com/yoitsro/joigoose",
  "engines": {
    "node": ">=9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hapi/hoek": "^8.5.0",
    "@hapi/joi": "^16.1.7"
  },
  "devDependencies": {
    "@hapi/code": "^7.0.0",
    "@hapi/lab": "^21.0.0",
    "@semantic-release/git": "^7.0.18",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "mongoose": "^5.8.0",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.31"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
