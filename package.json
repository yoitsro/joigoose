{
  "name": "joigoose",
  "version": "8.0.2",
  "description": "Joi validation for your Mongoose models without the hassle of maintaining two schemas",
  "main": "lib/index.js",
  "scripts": {
    "test": "lab test -c -I queueMicrotask",
    "release": "standard-version",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoitsro/joigoose"
  },
  "keywords": [
    "joi",
    "mongoose",
    "validation",
    "schema",
    "model",
    "hapi"
  ],
  "author": "Ro Ramtohul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoitsro/joigoose/issues"
  },
  "homepage": "https://github.com/yoitsro/joigoose",
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hapi/hoek": "^11.0.2",
    "joi": "^17.3.0"
  },
  "devDependencies": {
    "@hapi/code": "^9.0.3",
    "@hapi/lab": "^25.1.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mongoose": "^7.0.1",
    "prettier": "^2.8.4",
    "semantic-release": "^20.1.1"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}